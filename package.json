{
  "name": "transcriber",
  "version": "1.0.0",
  "description": "TypeScript Node.js Express server with YouTube transcript functionality",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec 'node --loader ts-node/esm' src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "typescript",
    "express",
    "node",
    "youtube",
    "transcript"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "eslint": "^9.39.2",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4"
  },
  "dependencies": {
    "@types/cors": "^2.8.19",
    "@types/node": "^25.0.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
